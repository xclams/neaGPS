<content>#set($stream_name="AdWordsJump")
#if($request.flag)
    #if($request.flag == "clear")
        #if($request.logpage &amp;&amp; $request.logpage != "")
            #set($log_infos=$xwiki.activitystream.searchEvents("act.stream='${stream_name}' and act.type='Log' and act.page='${request.logpage}'",false,0,0))
        #else
            #set($log_infos=$xwiki.activitystream.searchEvents("act.stream='${stream_name}' and act.type='Log'",false,0,0))
        #end
        $xwiki.activitystream.deleteActivityEvents($log_infos)
    #elseif($request.flag == "delete" &amp;&amp; $request.eventId)
        #set($log_infos=$xwiki.activitystream.searchEvents("act.eventId='${request.eventId}'",false,0,0))
        #if($log_infos.size()==1)
        $xwiki.activitystream.deleteActivityEvent($log_infos.get(0))
        #end
    #end
#end
#macro(pageViewLinks $pageN $totalP $pag)
    #if($totalP &gt; 0)
    &lt;div class="pagedNavigation" align="right"&gt;
    &lt;div align="right"&gt;
    #if($pag &gt; 0)
        #set($prevPage = $pag - 1)
        &lt;span class="pagedLink"&gt;&lt;a href="#" onclick="javascript:goto('$pageN', '0');"&gt;&lt;&lt;&lt;/a&gt;&lt;/span&gt;
        &lt;span class="pagedLink"&gt;&lt;a href="#" onclick="javascript:goto('$pageN', '$prevPage');"&gt;&lt;&lt;/a&gt;&lt;/span&gt;
    #else
        &lt;span class="pagedLink inactive"&gt;&lt;&lt;&lt;/span&gt;
        &lt;span class="pagedLink inactive"&gt;&lt;&lt;/span&gt;
    #end
    #set($displayCrtPageNumber = $pag + 1)
    #set($displayTotalPages = $totalP + 1)
        &lt;span class="pagedStatus"&gt;$displayCrtPageNumber / $displayTotalPages&lt;/span&gt;
    #if($pag &lt; $totalP)
        #set($nextPage = $pag + 1)
        &lt;span class="pagedLink"&gt;&lt;a href="#" onclick="javascript:goto('$pageN', '$nextPage');"&gt;&gt;&lt;/a&gt;&lt;/span&gt;
        &lt;span class="pagedLink"&gt;&lt;a href="#" onclick="javascript:goto('$pageN', '$totalP');"&gt;&gt;&gt;&lt;/a&gt;&lt;/span&gt;
    #else
        &lt;span class="pagedLink inactive"&gt;&gt;&lt;/span&gt;
        &lt;span class="pagedLink inactive"&gt;&gt;&gt;&lt;/span&gt;
    #end
    &lt;/div&gt;
    &lt;/div&gt;
    #end
#end
{pre}
&lt;script type="text/javascript" language="javascript"&gt;
function goto(pageName, page){
    document.getElementById(pageName).value = page;
    document.pageForm.submit();
}
function switchDivStatus(show_div){
    if(show_div.style.visibility == "visible" || show_div.style.display == "block"){
        show_div.style.visibility = "hidden";
        show_div.style.display = "none";
    }else{
        show_div.style.visibility = "visible";
        show_div.style.display = "block";
    }
}
&lt;/script&gt;
{/pre}
##=================== get all log paging parameters
#set($page = 0)
#if($request.page)
  #set($page = $util.parseInt($request.page))
#end
##=================== search all log count of pages
#set($hql_where = "where doc.parent='AdWordsJump.LandingPageLog'")
#set($count = $xwiki.search("select count(*) from XWikiDocument doc $hql_where").get(0))
&lt;form name='pageForm' method='post'&gt;
	&lt;input type='hidden' name='page' id='page' value='$!page'&gt;
&lt;/form&gt;
##=================== set all log paging values
#set($itemsPerPage = 5)
#set($startAt = $itemsPerPage * $page)
#if($startAt &lt; 0)
  #set($startAt = 0)
#end
#set($endAt = $itemsPerPage * ($page + 1))
#if($endAt &gt; $count)
  #set($endAt = $count)
#end
#if($startAt &gt; $endAt)
  #set($startAt = $endAt)
#end
#set($totalPages = ($count - 1) / $itemsPerPage)
##=================== show content
&lt;div align="right"&gt;
&lt;a href="$xwiki.getURL("AdWordsJump.LandingPageLogExport", "view","docname=all_log_docs&amp;xpage=popup")" target="_blank"&gt;Export All The Logs&lt;/a&gt;
&lt;span&gt; | &lt;/span&gt;
&lt;a href="javascript:if(window.confirm('Are you sure to clear all the logs?')) location.href='$doc.getURL($context.action, "flag=clear&amp;amp;page=$page")';"&gt;Clear All The Logs&lt;/a&gt;
&lt;/div&gt;
#set ($items = $xwiki.searchDocuments($hql_where, $itemsPerPage, $startAt))
#set($bar_title="Log")
##=================== show sub log page content
#foreach ($item in $items)
    #set($log_infos=$xwiki.search("from ActivityEventImpl as act where act.stream='${stream_name}' and act.type='Log' and act.page='${item}' order by param1",$itemsPerPage,0))
    #set($bentrydoc = $xwiki.getDocument($item))
    #subtitlebar(${bentrydoc.name} "View" ${bentrydoc.getURL()} "purple")
    #foreach ($log_info in $log_infos)
        #set($searchTime=$log_info.param1)
        #set($obj_title=$bar_title+":&amp;nbsp;&amp;nbsp;"+$searchTime)
&lt;div onclick="switchDivStatus(document.getElementById('div_${bentrydoc.name}_${velocityCount}'))" style="cursor:pointer"&gt;
        #set($del_url=$doc.getURL("view","flag=delete&amp;amp;eventId=${log_info.eventId}&amp;amp;page=$page"))
        #set($del_url="javascript:if(window.confirm('Are you sure to delete it?')) location.href='$del_url';")
        #titlebar($obj_title "DEL" $del_url "purple")
&lt;/div&gt;
&lt;div id="div_${bentrydoc.name}_${velocityCount}" style="visibility:hidden;display:none;"&gt;
&lt;table border="0" cellspacing="0" width="100%"&gt;
    &lt;tr&gt;&lt;td width="150"&gt;&lt;b&gt;Email:&lt;/b&gt;&lt;/td&gt;&lt;td&gt;$!log_info.param4&lt;/td&gt;&lt;/tr&gt;
    &lt;tr&gt;&lt;td&gt;&lt;b&gt;Search Term:&lt;/b&gt;&lt;/td&gt;&lt;td&gt;$!log_info.param2&lt;/td&gt;&lt;/tr&gt;
    &lt;tr&gt;&lt;td&gt;&lt;b&gt;Date Created:&lt;/b&gt;&lt;/td&gt;&lt;td&gt;$!searchTime&lt;/td&gt;&lt;/tr&gt;
    &lt;tr&gt;&lt;td&gt;&lt;b&gt;Is Permission:&lt;/b&gt;&lt;/td&gt;&lt;td&gt;$!log_info.param3&lt;/td&gt;&lt;/tr&gt;
    #set($to_trim = $bentrydoc.name.length() - 3)
    &lt;tr&gt;&lt;td&gt;&lt;b&gt;Landing Page:&lt;/b&gt;&lt;/td&gt;&lt;td&gt;${bentrydoc.name.substring(0, $to_trim)}&lt;/td&gt;&lt;/tr&gt;
&lt;/table&gt;
&lt;/div&gt;
    #end
    &lt;div style="float:left;"&gt;
    &lt;a href="$xwiki.getURL("AdWordsJump.LandingPageLogExport", "view","docname=${bentrydoc.fullName}&amp;xpage=popup")" target="_blank"&gt;Export&lt;/a&gt;
    &lt;span&gt; | &lt;/span&gt;
    &lt;a href="javascript:if(window.confirm('Are you sure to clear logs?')) location.href='$doc.getURL($context.action, "flag=clear&amp;amp;logpage=$item&amp;amp;page=$page")';"&gt;Clear&lt;/a&gt;
    &lt;/div&gt;
    &lt;div style="float:right;"&gt;
    &lt;a href="${bentrydoc.getURL()}" target="_blank"&gt;more...&lt;/a&gt;
    &lt;/div&gt;
&lt;br/&gt;
#end
#pageViewLinks("page" $totalPages $page)</content></xwikidoc>
