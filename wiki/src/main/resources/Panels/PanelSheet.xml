  &lt;tr&gt;
    &lt;td class="label"&gt;&lt;label&gt;#displayPropName($class.get("content")):&lt;/label&gt;&lt;/td&gt;
##    &lt;td&gt;$doc.display("content", $obj)&lt;/td&gt;
